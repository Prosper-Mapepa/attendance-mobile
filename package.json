{
  "name": "attendiq-mobile",
  "version": "1.0.1",
  "packageManager": "npm@10.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update",
    "postinstall": "node -e \"console.log('Postinstall: expo binary available at', require('path').join(process.cwd(), 'node_modules', '.bin', 'expo'))\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "axios": "^1.11.0",
    "expo": "~54.0.0",
    "expo-asset": "^12.0.7",
    "expo-camera": "^17.0.7",
    "expo-device": "~8.0.10",
    "expo-font": "^14.0.7",
    "expo-linear-gradient": "~15.0.6",
    "expo-local-authentication": "~15.0.0",
    "expo-location": "^19.0.6",
    "expo-navigation-bar": "^5.0.10",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~3.0.7",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "expo-updates": "~0.28.11",
    "@expo/cli": "~54.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "expo-dev-client": "~6.0.20",
    "typescript": "~5.9.2"
  },
  "private": true
}
