{
  "expo": {
    "name": "AttendIQ",
    "slug": "attendiq",
    "version": "1.0.8",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B0000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prospermap.attendiq",
      "buildNumber": "6",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to scan QR codes for attendance marking.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to verify classroom attendance.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to verify classroom attendance."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "scheme": "attendiq"
    },
    "android": {
      "package": "com.prospermap.attendiq",
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8B0000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.attendiq.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "attendiq",
              "host": "reset-password"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5e792208-6678-4813-ae6d-e0347287e75d"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/5e792208-6678-4813-ae6d-e0347287e75d",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
